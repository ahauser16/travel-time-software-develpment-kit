"$schema"   = "http://cyaninc.com/json-schemas/tosca-lite-v1/definition-module#"
title       = "traveltimeplatform ResponseShape"
package     = traveltimeplatform
version     = "1.0"
description = "Defines a ResponseShape"
authors     = ["F. Bar (foo@bar.baz)"]

imports {
  Root = tosca.resourceTypes.Root
  Coords = traveltimeplatform.resourceTypes.Coords
}
resourceTypes {

  ResponseShape {
    title = ResponseShape
    description = ResponseShape
    derivedFrom = Root
    properties {
        shell {
          type = array
          items.type = Coords
          description = ""
          optional = true
        }
        holes {
          type = array
          items.type = List[Coords]
          description = ""
          optional = true
        }
    }
  }
}

serviceTemplates {

  ResponseShape {
    title = ResponseShape
    description = ResponseShape
    implements = traveltimeplatform.resourceTypes.ResponseShape

  }
}
